@model Proyecto_Ajedrez_v_1.Models.urlLista

@{
    ViewBag.Title = "Index";
}

<h2>Webs de Ajedrez</h2>



<table>
    @using (Html.BeginForm("Index", "Scraper1"))
    {


        for (int i = 0; i < Model.webs.Count; i++)
        {



            <tr>
                <td>

                    @Html.CheckBoxFor(m => Model.webs[i].IsCheck)
                    @Model.webs[i].urlWeb
                    @Html.HiddenFor(m => Model.webs[i].id)
                    @Html.HiddenFor(m => Model.webs[i].urlWeb)
                </td>

            </tr>
        }
        <br />
        <input type="submit" value="Guardar cambios" />
        <p>
            @Html.ActionLink("Listado de Noticias", "Consultaweb2");
        </p>
        <br />

    }

</table>

<br />
<br />

<p>@ViewBag.selectFruit</p>

